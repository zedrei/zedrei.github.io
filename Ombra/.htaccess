# Cache
<IfModule mod_expires.c>
	ExpiresActive on
	ExpiresDefault A432000
</IfModule>

# .htaccess
<Files .htaccess>
order allow,deny
deny from all
</Files>

# Security
Options +FollowSymlinks -Indexes -MultiViews 
AddDefaultCharset UTF-8

# Error Pages
ErrorDocument 403 /404.shtml
ErrorDocument 404 /404.shtml 
ErrorDocument 500 /500.shtml 
ErrorDocument 503 /503.shtml 

# Timezone
SetEnv TZ US/Eastern

RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^www\.ombracustomblinds\.com$
RewriteRule ^/?$ "https\:\/\/ombracustomblinds\.com\/" [R=301,L]

